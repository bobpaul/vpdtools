<?xml version="1.0"?>
<!--myl:  2021-09-21:  Create.-->

<record name="LXR0">
  <rdesc>The LXR0 record:  Load ID record</rdesc>

   <keyword name="RT">
      <kwdesc>The Record Type keyword</kwdesc>
      <kwformat>ascii</kwformat>
      <kwlen>4</kwlen>
      <kwdata>LXR0</kwdata>
   </keyword>

   <keyword name="VZ">
      <kwdesc>Record Version</kwdesc>
      <kwformat>ascii</kwformat>
      <kwlen>2</kwlen>
      <!--Increment when the record is changed.-->
      <kwdata>01</kwdata>
   </keyword>

   <keyword name="LX">
      <kwdesc>Load ID</kwdesc>
      <kwformat>hex</kwformat>
      <kwlen>8</kwlen>
      <!-- Value to be provided by Hypervisor (phyp) firmware team -->
      <kwdata>00</kwdata>
   </keyword>

</record>

